{
  "name": "gamexhub",
  "version": "1.0.0",
  "description": "Added the branch develop",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/*.ts",
    "lint:fix": "eslint ./src/*.ts --fix",
    "prettier:check": "prettier -c ./src/*.ts",
    "prettier:fix": "prettier --write ./src/*.ts",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "npm run prettier:check",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amithalex/gameXHub.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amithalex/gameXHub/issues"
  },
  "homepage": "https://github.com/amithalex/gameXHub#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1"
  },
    "engines": {
    "node": ">=10.0.0"
  }
}
